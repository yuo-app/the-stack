{
  "name": "sqlite-wasm-drizzle-tauri",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.2.18",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prebuild": "bun ./scripts/compile-migrations.ts",
    "dev": "bun run prebuild && vinxi dev --host",
    "build": "vinxi build",
    "check": "tsc --noEmit --skipLibCheck",
    "prepreview": "bun scripts/copy-dev-vars.mjs",
    "preview": "wrangler dev --cwd .output",
    "l:generate": "drizzle-kit generate --config=drizzle.local.config.ts",
    "l:studio": "drizzle-kit studio --config=drizzle.local.config.ts",
    "r:generate": "drizzle-kit generate --config=drizzle.remote.config.ts",
    "r:studio": "drizzle-kit studio --config=drizzle.remote.config.ts",
    "r:migrate": "drizzle-kit migrate --config=drizzle.remote.config.ts",
    "lint": "eslint . --fix",
    "tauri": "tauri"
  },
  "dependencies": {
    "@auth/core": "0.40.0",
    "@auth/solid-start": "^0.18.0",
    "@libsql/client": "^0.15.9",
    "@solid-mediakit/auth": "^3.1.3",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.6",
    "dotenv": "^17.1.0",
    "drizzle-orm": "^0.44.2",
    "solid-js": "^1.9.7",
    "sqlocal": "^0.14.1",
    "unenv": "^1.10.0",
    "vinxi": "^0.5.8",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.16.2",
    "@tauri-apps/cli": "^2.6.2",
    "@types/bun": "^1.2.18",
    "@types/node": "^24.0.10",
    "@unocss/eslint-plugin": "66.3.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "9.30.1",
    "eslint-plugin-solid": "0.14.5",
    "typescript": "5.8.3",
    "unocss": "66.3.3",
    "wrangler": "^4.23.0"
  },
  "overrides": {
    "vite": "5.4.10"
  }
}
